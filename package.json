{
  "name": "retext-preset-wooorm",
  "version": "1.0.1",
  "description": "Personal prose style",
  "license": "MIT",
  "keywords": [
    "retext",
    "preset",
    "wooorm"
  ],
  "repository": "wooorm/retext-preset-wooorm",
  "bugs": "https://github.com/wooorm/retext-preset-wooorm/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "retext-contractions": "^2.1.0",
    "retext-diacritics": "^1.2.0",
    "retext-indefinite-article": "^1.1.0",
    "retext-quotes": "^2.0.0",
    "retext-redundant-acronyms": "^1.2.0",
    "retext-repeated-words": "^1.2.0",
    "retext-sentence-spacing": "^2.0.0"
  },
  "devDependencies": {
    "prettier": "^1.12.1",
    "remark-cli": "^6.0.0",
    "remark-preset-wooorm": "^4.0.0",
    "xo": "^0.24.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier --write \"**/*.js\" && xo --fix",
    "test": "npm run format"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "no-var": "off",
      "prefer-arrow-callback": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
